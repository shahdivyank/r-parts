generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("NEXT_PUBLIC_DATABASE_URL")
}

// Don't touch the above

model Test {
  id      BigInt  @id @default(sequence())
  name    String?
}

